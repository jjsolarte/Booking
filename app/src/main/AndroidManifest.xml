<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="co.jjsolarte.booking">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
    <uses-feature android:name="android.hardware.camera2"
        android:required="false"></uses-feature>

    <application
        android:name=".MyApply"
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="BookSena"
        android:roundIcon="@drawable/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".TomarFoto"></activity>
        <activity android:name=".ContainerActivity" />
        <activity
            android:name=".AcercaDe"
            android:parentActivityName=".Menu">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".Menu" />
        </activity>
        <activity
            android:name=".Menu"
            android:theme="@style/AppBooking" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppBooking">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>